{
    "name": "xaraya/modules",
    "description": "Collection of modules to extend Xaraya's functionality",
    "license": "GPL-2.0-or-later",
    "extra": {
        "branch-alias": {
            "dev-master": "2.4.x-dev"
        }
    },
    "require": {},
    "suggest": {},
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
